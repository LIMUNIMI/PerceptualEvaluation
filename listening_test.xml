<?xml version="1.0" encoding="ISO-8859-1"?>
<waet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="test-schema.xsd">
    <setup interface="discrete" projectReturn="save.php" returnURL="" randomiseOrder="true" poolSize="7" loudness="-23" crossFade="1" randomiseAxisOrder="false" preSilence="1" postSilence="1" playOne="false" minNumberPlays="0">
        <exitText>Thank you for participating in this listening test. You may now exit the test environment.

            If you have any suggestions or comment, please report it to [federico.simonetta@unimi.it](mailto:federico.simonetta@unimi.it).</exitText>
        <calibration checkFrequencies="true" checkLevels="true" checkChannels="false"/>
        <survey location="pre" showBackButton="true">
            <surveyradio id="expertise" mandatory="true">
                <statement>Have you ever studied music?</statement>
                <option name="0">I have never studied music</option>
                <option name="1">I study/have studied music for hobby</option>
                <option name="2">I study/have studied music professionally for a few years</option>
                <option name="3">I have a professional degree in music</option>
                <option name="4">I work as professional musician</option>
            </surveyradio>
            <surveyradio id="habits_classical" mandatory="true">
                <statement>How often do you listen to **classical** music usually?</statement>
                <option name="0">never</option>
                <option name="1">less than 1 hour per week</option>
                <option name="2">between 1 and 3 hours per week</option>
                <option name="3">between 3 and 6 hours per week</option>
                <option name="4">more than 6 hours per week</option>
            </surveyradio>
            <surveyradio id="habits_general" mandatory="true">
                <statement>How often do you listen to **any genre** of music **except classical music** usually?</statement>
                <option name="0">never</option>
                <option name="1">less than 1 hour per week</option>
                <option name="2">between 1 and 3 hours per week</option>
                <option name="3">between 3 and 6 hours per week</option>
                <option name="4">more than 6 hours per week</option>
            </surveyradio>
        </survey>
        <survey location="post" showBackButton="true">
            <surveyquestion id="comments" boxsize="normal">
                <statement>If you have comments about this test, please insert them here &lt;3</statement>
            </surveyquestion>
        </survey>
        <metric>
            <metricenable>testTimer</metricenable>
            <metricenable>elementTimer</metricenable>
            <metricenable>elementFlagMoved</metricenable>
        </metric>
        <interface>
            <interfaceoption name="fragmentPlayed" type="check"/>
            <interfaceoption name="fragmentFullPlayback" type="check"/>
            <interfaceoption name="volume" type="show"/>
            <interfaceoption name="ticks" type="show"/>
            <interfaceoption name="playhead" type="show"/>
            <interfaceoption name="page-count" type="show"/>
        </interface>
    </setup>
    <page id="training" hostURL="training/" randomiseOrder="true" repeatCount="0" loop="false" synchronous="false" label="capital" labelStart="" poolSize="0" alwaysInclude="false" position="0" preSilence="0" postSilence="0" playOne="false" restrictMovement="false" minNumberPlays="0">
        <commentboxprefix>Comment on track</commentboxprefix>
        <interface name="interpretation">
